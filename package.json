  "engineStrict": true,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "postinstall": "npm run bootstrap",
    "bootstrap": "npm run install-solc-0.4 && npm run install-solc-0.5",
    "install-solc-0.4": "cd ./lib/compile/compilers/solc-0.4 && npm install",
    "install-solc-0.5": "cd ./lib/compile/compilers/solc-0.5 && npm install",
    "clean-solc-0.4": "rm -rf ./lib/compile/compilers/solc-0.4/node_modules",
    "clean-solc-0.5": "rm -rf ./lib/compile/compilers/solc-0.5/node_modules",
    "test": "mocha"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "chalk": "^2.4.2",
    "crypto-js": "^3.1.9-1",
    "deepcopy": "^2.0.0",
    "ethereumjs-util": "^6.1.0",
    "ethers": "^4.0.47",
    "ethjs-abi": "^0.2.1",
    "fs-extra": "^8.0.1",
    "isarray": "^2.0.4",
    "keccak": "^2.0.0",
    "node-forge": "^0.10.0",
    "pem-file": "^1.0.1",
    "rlp": "^2.2.3",
    "secp256k1": "^3.7.1",
    "semver": "^7.1.1",
    "tls": "^0.0.1",
    "uuid": "^3.3.2",
    "web3": "^1.2.0"
  },
  "devDependencies": {
    "mocha": "^7.1.2"
  },
